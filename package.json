{
  "name": "azure-service-bus-explorer",
  "displayName": "Azure Service Bus Explorer",
  "description": "Azure Servuce Bus Explorer",
  "publisher": "teunmooij",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:azure-service-bus-explorer.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "azure-service-bus-explorer.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "service-bus-explorer",
          "title": "Azure Service Bus Explorer",
          "icon": "resources/activity-bar-icon.svg"
        }
      ]
    },
    "views": {
      "service-bus-explorer": [
        {
          "id": "namespaces",
          "name": "Namespaces"
        },
        {
          "id": "queues",
          "name": "Queues"
        },
        {
          "id": "topics",
          "name": "Topics"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "namespaces",
        "contents": "[Add connection](command:azure-service-bus-explorer.helloWorld)"
      },
      {
        "view": "queues",
        "contents": "Select a service bus connection"
      },
      {
        "view": "topics",
        "contents": "Select a service bus connection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "typescript": "^4.7.4"
  }
}
